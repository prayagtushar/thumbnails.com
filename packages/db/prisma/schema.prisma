generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ThumbnailJob {
  id           String   @id @default(cuid())
  videoTitle   String
  niche        String?
  referenceSet String
  personaId    String
  concepts     String
  createdAt    DateTime @default(now())
}
